    "build": "tsc"
  },
  "author": "Stefan Poindl",
  "license": "MIT",
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@rollup/plugin-commonjs": "^24.0.1",
    "typescript": "^4.9.5",
    "axios": "^1.3.3"
  }
}
