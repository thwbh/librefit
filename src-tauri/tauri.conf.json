{
	"$schema": "https://schema.tauri.app/config/2",
	"app": {
		"security": {
			"csp": "default-src 'self'; script-src 'self'; img-src 'self' data:; style-src 'self' 'unsafe-inline';"
		},
		"windows": [
			{
				"fullscreen": false,
				"height": 800,
				"resizable": true,
				"title": "librefit",
				"width": 420
			}
		]
	},
	"build": {
		"beforeBuildCommand": "cargo tauri-typegen generate && npm run build",
		"beforeDevCommand": "npm run dev",
		"devUrl": "http://localhost:1420",
		"frontendDist": "../build"
	},
	"bundle": {
		"active": true,
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		],
		"targets": "all"
	},
	"identifier": "io.tohowabohu.librefit",
	"plugins": {
		"typegen": {
			"excludePatterns": null,
			"includePatterns": null,
			"includePrivate": false,
			"outputPath": "../src/lib/api/gen",
			"projectPath": ".",
			"typeMappings": null,
			"validationLibrary": "zod",
			"verbose": false,
			"visualizeDeps": false
		}
	},
	"productName": "librefit",
	"version": "25.49.0"
}
